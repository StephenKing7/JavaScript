/**
 * @param {string} s
 * @return {number}
 */
var romanToInt = function (s) {
    // create a dictionary to convert the Roman Numeral to a digit
    const symbolToValue = {
        I: 1,
        V: 5,
        X: 10,
        L: 50,
        C: 100,
        D: 500,
        M: 1000
    };

// initialize the total variable for the output
var total = 0;

// adjust the string so that the values are added from left to right.  in other words, we don't need to determine whether we add or subtract on the fly
s = s.replace("IV", "IIII");
s = s.replace("IX", "VIIII");
s = s.replace("XL", "XXXX");
s = s.replace("XC", "LXXXX");
s = s.replace("CD", "CCCC");
s = s.replace("CM", "DCCCC");

// loop through the string values and add them to the total
for (var i = 0; i < s.length; i++) {
    var keyName = s[i];
    var keyNameValue = symbolToValue[keyName];
        total += keyNameValue;
}

return total;

};

romanToInt("III");
